
    <div class="header-image text-center">
      <h1 class= 'header-title'>Welcome on Eventbrite</h1>
    </div>
    <div class="container">
      <div class="row">
        <% @events.each do |event| %>
          <div class="col-sm-6">
            <div>
              <div>
                <ul type="circle">
                  <li><%= link_to event.title, event_path(event.id) %></li>
                </ul>
                <p><%= event.start_date %></p>
                <p><%= event.location %></p>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>

